{
  "name": "@minsoochoo/audio-player",
  "version": "0.3.1",
  "description": "SvelteKit audio player for your music, with custom controls, playlist, filters, and search functionality.",
  "keywords": [
    "svelte",
    "sveltekit",
    "audio",
    "player",
    "mp3",
    "music"
  ],
  "homepage": "https://www.npmjs.com/package/@minsoochoo/audio-player",
  "author": "Minsoo Choo <minsoochoo0122@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fel1x-developer/audio-player.git"
  },
  "bugs": {
    "url": "https://github.com/fel1x-developer/audio-player/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && bun run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest run",
    "test:headless": "vitest run --browser.headless",
    "test:watch": "vitest",
    "test:watch:headless": "vitest --browser.headless",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.0",
    "@playwright/test": "^1.53.2",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-svelte-csf": "^5.0.4",
    "@storybook/sveltekit": "^9.0.15",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/package": "^2.3.12",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.34",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-storybook": "^9.0.15",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "oxlint": "^1.4.0",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "publint": "^0.3.12",
    "storybook": "^9.0.15",
    "svelte": "^5.34.9",
    "svelte-check": "^4.2.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "npm:rolldown-vite@6.3.21",
    "vitest": "^3.2.4",
    "vitest-browser-svelte": "^1.0.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@6.3.21"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  }
}