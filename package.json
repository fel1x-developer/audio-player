{
  "name": "@minsoochoo/audio-player",
  "version": "0.2.0",
  "description": "SvelteKit audio player for your music, with custom controls, playlist, filters, and search functionality.",
  "keywords": [
    "svelte",
    "sveltekit",
    "audio",
    "player",
    "mp3",
    "music"
  ],
  "homepage": "https://www.npmjs.com/package/@minsoochoo/audio-player",
  "author": "Minsoo Choo <minsoochoo0122@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fel1x-developer/audio-player.git"
  },
  "bugs": {
    "url": "https://github.com/fel1x-developer/audio-player/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && bun run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest run",
    "test:headless": "vitest run --browser.headless",
    "test:watch": "vitest",
    "test:watch:headless": "vitest --browser.headless",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-svelte-csf": "^5.0.0-next.28",
    "@storybook/blocks": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/svelte": "^8.6.12",
    "@storybook/sveltekit": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.14.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.11",
    "storybook": "^8.6.12",
    "svelte": "^5.25.12",
    "svelte-check": "^4.1.6",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.2.6",
    "vitest": "^3.1.1",
    "vitest-browser-svelte": "^0.1.0"
  }
}
